<%# Add conditionals to check if user has the specified social media account before %>

<div class="container">
  <div class="row m-0 float-right">
    <div class="col-lg-2 col-md-2 col-sm-4 col-xs-4">
      <%= link_to(image_tag("soundcloud_icon.png", size: "50x50", class: "rounded"), "http://www.soundcloud.com") %>
    </div>
    <div class="col-lg-2 col-md-2 col-sm-4 col-xs-4">
      <%= link_to(image_tag("facebook_icon.png", size: "50x50", class: "rounded"), "http://www.facebook.com") %>
    </div>
    <div class="col-lg-2 col-md-2 col-sm-4 col-xs-4">
      <%= link_to(image_tag("twitter_icon.png", size: "50x50", class: "rounded"), "http://www.twitter.com") %>
    </div>
    <div class="col-lg-2 col-md-2 col-sm-4 col-xs-4">
      <%= link_to(image_tag("instagram_icon.png", size: "50x50", class: "rounded"), "http://www.instagram.com") %>
    </div>
  </div>
</div>